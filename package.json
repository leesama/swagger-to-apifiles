{
  "name": "openapi-generate-config",
  "version": "3.0.6",
  "description": "run openapi-typescript by config",
  "main": "dist/index.js",
  "bin": {
    "openapi-make": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "keywords": ["swagger","umi","request"],
  "nodemonConfig": {
    "ignore": [
      "src/types/*",
      "src/services/*",
      "tsconfig.json",
      "services.d.ts"
    ]
  },
  "scripts": {
    "build": "npm run clean  && npm run copy-files && tsc",
    "build-test": "npm run clean  && npm run copy-files && tsc && node dist/index.js",
    "clean": "fse remove dist/ && fse remove generateTypeBySwagger.json && fse remove src/services/ && fse remove src/types/ && fse remove services.d.ts",
    "copy-files": "fse copy  ./src/templates ./dist/templates",
    "dev": "nodemon  --watch ./ --exec ts-node src/index.ts",
    "publish": "npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leesama/openapi-generate-config.git"
  },
  "author": "leesher",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/leesama/openapi-generate-config/issues"
  },
  "homepage": "https://github.com/leesama/openapi-generate-config#readme",
  "devDependencies": {
    "@types/fs-extra": "^9.0.11",
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.6.1",
    "@types/request": "^2.48.5",
    "@types/write": "^2.0.0",
    "nodemon": "^2.0.7",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@atao60/fse-cli": "^0.0.56",
    "chalk": "^4.1.1",
    "change-case": "^4.1.2",
    "fs-extra": "^10.0.0",
    "got": "^11.8.2",
    "lodash": "^4.17.21",
    "openapi-typescript": "^3.3.1",
    "read-pkg-up": "^7.0.1",
    "rimraf": "^3.0.2"
  }
}
