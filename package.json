{
  "name": "swagger-to-apifiles",
  "author": "leesher",
  "version": "1.3.0",
  "description": "make api files by swagger",
  "main": "dist/index.js",
  "bin": {
    "make-api": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "swagger",
    "umi",
    "request"
  ],
  "nodemonConfig": {
    "ignore": [
      "src/types/*",
      "src/services/*",
      "src/api/*"
    ]
  },
  "scripts": {
    "build": "npm run clean  && tsc  && npm run copy-files",
    "build-test": "npm run clean  && npm run copy-files && tsc && node dist/index.js",
    "clean": "fse remove dist/ && fse remove generateTypeBySwagger.json && fse remove src/services/ && fse remove src/types/",
    "copy-files": "fse copy  ./src/templates ./dist/templates",
    "dev": "nodemon  --watch ./src --exec ts-node src/index.ts",
    "pub": "npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leesama/openapi-generate-config.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/leesama/openapi-generate-config/issues"
  },
  "homepage": "https://github.com/leesama/openapi-generate-config#readme",
  "devDependencies": {
    "@types/ejs": "^3.0.6",
    "@types/fs-extra": "^9.0.11",
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.6.1",
    "@types/pascalcase": "^1.0.0",
    "@types/request": "^2.48.5",
    "@types/write": "^2.0.0",
    "nodemon": "^2.0.7",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@atao60/fse-cli": "^0.0.56",
    "chalk": "^4.1.1",
    "ejs": "^3.1.6",
    "fs-extra": "^10.0.0",
    "got": "^11.8.2",
    "lodash": "^4.17.21",
    "openapi-typescript": "^3.3.1",
    "pascalcase": "^1.0.0"
  }
}
