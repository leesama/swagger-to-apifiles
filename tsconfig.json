{
  "compilerOptions": {
    "declaration": true,
    "outDir": "./dist",
    "target": "es5",
    "module": "commonjs",
    "sourceMap": false,
    "downlevelIteration": true,
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["./dist/"]
}
