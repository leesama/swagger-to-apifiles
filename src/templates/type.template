import type {operations } from "./request"
// 请求的结果数据类型
export type ReqResType<T extends keyof operations> = Required<
operations[T]["responses"][200] extends {
    schema: infer S;
  }
    ? S
    : unknown
>;
// 请求的Data数据类型
export type ReqDataType<T extends keyof operations> = ReqResType<T> extends {
  data: infer D;
}
  ? D
  : any;
// 请求的参数类型
export type ReqParamsType<T extends keyof operations> = operations[T] extends {
  parameters: infer P;
}
  ? P extends {
      query: infer Q;
    }
    ? Q
    : P extends {
        body: infer B;
      }
    ? B[keyof B]
    : void
  : void;
